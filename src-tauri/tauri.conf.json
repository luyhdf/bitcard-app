{
  "build": {
    "beforeDevCommand": "npm install",
    "beforeBuildCommand": "npm install",
    "devPath": "../file_reader.html",
    "distDir": "../",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "file-reader",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true,
        "readFile": true,
        "writeFile": true,
        "readDir": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.file.reader",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "文件读写示例",
        "width": 800,
        "height": 600
      }
    ]
  }
} 